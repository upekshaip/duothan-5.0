generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/duothan"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  gender    String?
  password  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}